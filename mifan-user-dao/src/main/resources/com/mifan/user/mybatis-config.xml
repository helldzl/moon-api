<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="cacheEnabled" value="true"/>
        <setting name="lazyLoadingEnabled" value="true"/>
        <setting name="multipleResultSetsEnabled" value="true"/>
        <setting name="useColumnLabel" value="true"/>
        <setting name="useGeneratedKeys" value="false"/>
        <setting name="autoMappingBehavior" value="PARTIAL"/>
        <setting name="defaultExecutorType" value="SIMPLE"/>
        <setting name="defaultStatementTimeout" value="25"/>
        <setting name="defaultFetchSize" value="100"/>
        <setting name="safeRowBoundsEnabled" value="false"/>
        <setting name="mapUnderscoreToCamelCase" value="false"/>
        <setting name="localCacheScope" value="SESSION"/>
        <setting name="jdbcTypeForNull" value="OTHER"/>
        <setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString"/>
    </settings>

    <!-- user @Alias to rename -->
    <typeAliases>
        <package name="com.mifan.user.domain"/>
    </typeAliases>

    <mappers>
        <mapper resource="com/mifan/user/domain/AuthElementsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/AuthOperationsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/AuthoritiesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/GroupsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/GroupsRolesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/PersistentLoginsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/RolesAuthoritiesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/RolesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/SitesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UserAccountsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UserAddressesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UserAliasesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UserInvitationsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UserKarmaLogsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UserProfilesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UsersGroupsMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UsersMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/UsersRolesMapper.xml"></mapper>
        <mapper resource="com/mifan/user/domain/WxRedPacketMapper.xml"></mapper>
    </mappers>

</configuration>
